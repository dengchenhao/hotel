<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.integration.entity.Order" table="h_order">
		<id name="orderid" column="orderid" type="integer">
			<generator class="identity"> <!-- 主键id的生成方式为自增 -->
			</generator>
		</id>
		<property name="userid" column="userid" type="integer"></property>
		<property name="roomid" column="roomid" type="integer"></property>
		<property name="name" column="name" type="string"></property>
		<property name="price" column="price" type="string"></property>
		<property name="category" column="category" type="string"></property>
		<property name="username" column="username" type="string"></property>
		<property name="status" column="status" type="integer"></property>
		<property name="starttime" column="starttime" type="string"></property>
		<property name="money" column="money" type="string"></property>
		<property name="parkid" column="parkid" type="integer"></property>
		<property name="registertime" column="registertime" type="string"></property>
		
	</class>
</hibernate-mapping>